{
    "ref": "master",
    "path": "typings",
    "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "moduleResolution": "node",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "diagnostics": false,
      "removeComments": true,
      "noImplicitAny": false,
      "declaration": true,
      "noResolve": false,
      "noEmitHelpers": true,
      "outDir": "bin",
      "lib": ["es5", "es6", "dom"]
    },
    "exclude": [
      "node_modules",
      "typings/global",
      "typings/index.d.ts"
    ],
    "files": [
      "./lib/main.ts"
    ]
  }
